HOME = .
openssl_conf = openssl_init

[openssl_init]
providers = provider_sect
alg_section = alg_sect

[provider_sect]
fips = fips_sect
base = base_sect

[base_sect]
activate = 1

[fips_sect]
activate = 1
# Note: We do NOT specify 'module=' here. 
# We let Red Hat find the native RPM installed at /usr/lib64/ossl-modules/fips.so automatically.

[alg_sect]
default_properties = fips=yes
